{% extends 'base.html.twig' %}

{% block title %}KeyPair{% endblock %}

{% block body %}
    <h1>KeyPair</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ key_pair.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ key_pair.name }}</td>
            </tr>
            <tr>
                <th>Fingerprint</th>
                <td>{{ key_pair.fingerprint }}</td>
            </tr>
            <tr>
                <th>PublicKey</th>
                <td>{{ key_pair.publicKey }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('key_pair_index') }}">back to list</a>

    <a href="{{ path('key_pair_edit', {'id': key_pair.id}) }}">edit</a>

    {{ include('key_pair/_delete_form.html.twig') }}
{% endblock %}
